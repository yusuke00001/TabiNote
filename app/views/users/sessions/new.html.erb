<div class="users-sessions-new">
  <%= form_with(model: resource, as: resource_name, url: session_path(resource_name)) do |f| %>
    <div class="card-style">
      <h1 class="title"><%= t('.title')%></h1>
      <div class="form-group">
        <%= f.label :email %><br>
        <%= f.email_field :email, class:"text-field" %>
      </div>
      <div class="form-group">
        <%= f.label :password %><br>
        <%= f.password_field :password, class:"text-field"%>
      </div>
      <div class="submit">
        <%= f.submit t('.submit'), class:"submit-botton"%>
      </div>
      <div class="link">
        <%= render "users/shared/links" %>
      </div>
    </div>
  <% end %>
